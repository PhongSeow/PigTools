<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <RootNamespace>PigCmdLibDemo</RootNamespace>
    <TargetFrameworks>net20;net30;net40;net5.0;netcoreapp3.1;</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Demo console program for PigCmdLib.</Description>
    <Authors>Seow Phong</Authors>
    <Company>Seow Phong Studio</Company>
    <Copyright>Copyright (c) 2022 Seow Phong</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://en.seowphong.com/oss/PigTools/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PhongSeow/PigTools/tree/main/Src/DotNet/PigCmdLibDemo</RepositoryUrl>
    <Version>1.0.2</Version>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>Demo PigCmdLib .net develop shell command windows Linux VB6</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="PigCmdLib" Version="1.0.22" />
  </ItemGroup>

</Project>
